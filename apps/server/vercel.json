{
  "name": "inbrowser-api",
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "env": {
        "CANVAS_SKIP_INSTALL": "true"
      }
    }
  },
  "build": {
    "env": {
      "CANVAS_SKIP_INSTALL": "true"
    }
  }
}